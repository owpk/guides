else if(_this.finalEndVal!==null){_this.update(_this.finalEndVal);}
else{if(_this.callback){_this.callback();}}};this.formatNumber=function(num){var neg=(num<0)?'-':'';var result,x,x1,x2,x3;result=Math.abs(num).toFixed(_this.options.decimalPlaces);result+='';x=result.split('.');x1=x[0];x2=x.length>1?_this.options.decimal+x[1]:'';if(_this.options.useGrouping&&num>9999){x3='';for(var i=0,len=x1.length;i<len;++i){if(i!==0&&(i%3)===0){x3=_this.options.separator+x3;}
