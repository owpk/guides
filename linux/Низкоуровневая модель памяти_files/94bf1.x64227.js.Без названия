ю почту",bi="Баг отправлен на рассмотрение, спасибо",$i="Все поля должны быть заполнены",yi="Расскажите, что именно происходит.\nЕсли есть возможность, сделайте скриншоты или запишите видео.\nЗалейте их куда-нибудь и пришлите ссылку.",vi="Расскажите, как, по-вашему, это должно было работать.",wi="Максимально опишите окружение происходящего: ОС, браузер и т.д.",_i="Вы уверены что хотите отвязать",xi="Вы успешно отвязали",ki="Что-то пошло не так",Si="А как ты входить потом-то собираешься?",Li="Спасибо, ваши голоса приняты",Ei="Вы уверены, что сделали правильный выбор?",Pi="Добавить в закладки",Mi="Убрать из закладок",Ai=we([]),Ni=we(0),Ti=we(!0),Ii=we(!1);let zi;Ai.subscribe(e=>{if(e&&e.length>0){e.find(e=>e.isSelected)?Ti.set(!0):Ti.set(!1)}else Ti.set(!1);zi=e}),Ni.subscribe(e=>{});const Oi=()=>{Ai.update(e=>zi.map(e=>(e.isSelected=!1,e))),Ni.set(1)},Ci=async(e=!1)=>{try{if(e){const e=await(async()=>await Ce(rt))();e?0===e.length?Oi():(Ai.update(t=>zi.map(t=>{if(e){let n=e.find(e=>e.question===t.id);t.isSelected=!!n&&!t.isSelected}return t})),Ni.set(0)):Oi()}else Oi()}catch(e){Pe.log(e)}},Gi=async e=>{Ai.update(t=>zi.map(t=>(t.id===e&&(t.isSelected=!t.isSelected),t)))},Ri=async()=>{let e=(()=>{let e=[];return zi.map(t=>{t.isSelected&&e.push(t.id)}),e})();if(e.length>0){Ni.set(0),!0===await(async e=>await Re(ct,{questions:e}))(e)?alert(Li):await Ci(!0)}};function ji(e){let t,n,i=e[0]["creator.name"]+"";return{c(){t=S(i),n=S(" и проект")},m(e,i){v(e,t,i),v(e,n,i)},p(e,n){1&n&&i!==(i=e[0]["creator.name"]+"")&&A(t,i)},d(e){e&&w(t),e&&w(n)}}}function Ui(t){let n;return{c(){n=S("Проект")},m(e,t){v(e,n,t)},p:e,d(e){e&&w(n)}}}function Di(e){let t,n,i,o,s,l,a=ci+"",r=e[0].name+"";return{c(){t=x("button"),n=S(a),i=S(" «"),o=S(r),s=S("»"),M(t,"class","quiz-btn tx_rating_plus svelte-14hbtk0"),I(t,"disabled",e[1])},m(a,r,c){v(a,t,r),y(t,n),y(t,i),y(t,o),y(t,s),c&&l(),l=P(t,"click",e[3])},p(e,n){1&n&&r!==(r=e[0].name+"")&&A(o,r),2&n&&I(t,"disabled",e[1])},d(e){e&&w(t),l()}}}funct